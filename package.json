    "build:www": "vite build --outDir ../www/crm --base /assets/bizz_plus/crm/",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "tailwindcss": "^4.1.11",
    "vue": "^3.4.38",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.19",
    "vue-tsc": "^2.1.4"
  }
}
